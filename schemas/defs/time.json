{   
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://sukedachisan.github.io/json-schema/schemas/defs/time.json",
    "title": "TimeDefinitions",
    "description": "時間と分数で構成される時間定義",
    "$comment": "$defs内の各要素を$refで参照して利用してください。",
    "$defs": {
        "hour": {
            "$ref": "https://sukedachisan.github.io/json-schema/schemas/defs/core/datetime.json#/$defs/hour"
        },
        "minute": {
            "$ref": "https://sukedachisan.github.io/json-schema/schemas/defs/core/datetime.json#/$defs/minute"
        }
    }
}
